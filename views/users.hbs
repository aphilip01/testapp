<div class="container-fluid">
  <h2>List of customer requests</h2>
    

  <table class="table table-bordered">
    <thead>
      <tr>
        <th>name</th>
        <th>contact#</th>
        <th>Email</th>
        <th>Course</th>
        <th>message</th>
        <th>gender</th>
      </tr>
    </thead>
    
    <tbody>
      {{#each users}}  
      <tr>
        <td>{{name}}</td>
        <td>{{contactno}}</td>
        <td>{{email}}</td>
        <td>{{course}}</td>
        <td>{{message}}</td>
        <td>{{gender}}</td>
      </tr>
      {{/each}}
    </tbody>
    
  </table>

  

</div>

